<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <title>My Dashboard</title>
    </head>
    <body>
        <attribute name="class" value="account"/>
        <referenceBlock name="customer_account_navigation">
            <block class="Magento\Framework\View\Element\Html\Link\Current" name="magenest_movie_custom_my_account">
                <arguments>
                    <argument name="path" xsi:type="string">movie/account/custom</argument>
                    <argument name="label" xsi:type="string">My Dashboard</argument>
                    <argument name="sortOrder" xsi:type="number">150</argument>
                    <argument name="navigation" xsi:type="boolean">true</argument>
                </arguments>
            </block>
        </referenceBlock>
    </body>
</page>
